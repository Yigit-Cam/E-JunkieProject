<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">


<suite name="E-Junkie Test Suite"> <!--Cross browser icin -->
    <test name="ContactUsTestEdge">
        <parameter name="BrowserType" value="edge"></parameter>
        <classes>
            <class name="eJunkie.tests.ContactUsTest"></class>
        </classes>
    </test>
    <test name="ContactUsTestChrome">
        <parameter name="BrowserType" value="chrome"></parameter>
        <classes>
            <class name="eJunkie.tests.ContactUsTest"></class>
        </classes>
    </test>

</suite>